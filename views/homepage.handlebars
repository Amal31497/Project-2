{{!-- TODO Create a card for the chef of the week --}}
<div class="container-xl">
    <div class="row">
        <h2>AVAILABLE CHEFS IN YOUR NEIGHBORHOOD</h2>
    </div>
    <div class="row" id="chefs">
        {{#each chefs as |chef|}}
        {{> top-chefs}}
        {{/each}}
    </div>
</div>
<div class="container-xl">
    <div class="row" id="weekDishes">
        <h2>DISHES OF THE WEEK</h2>
    </div>
    <div class="row" id="dishes">
        {{#each chefs as |chef|}}
            {{#each cuisines as |cuisine|}}
                {{#each dishes as |dish|}}
                {{> top-dishes}}
                {{/each}}
            {{/each}}
        {{/each}}
    </div>
</div>