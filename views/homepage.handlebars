{{!-- TODO Create a card for the chef of the week --}}
<div class="container-xl">
    <div class="row" id="chefs">
        <h2 span>AVAILABLE CHEFS IN YOUR NEIGHBORHOOD</h2>
        {{#each chefs as |chef|}}
            {{first_name}}{{last_name}}
        {{/each}}
    </div>
</div>
<div class="container-xl">
    <div class="row" id="weekDishes">
        <h2>DISHES OF THE WEEK</h2>
        {{#each chefs as |chef|}}
            {{#each cuisines as |cuisine|}}
                {{#each dishes as |dish|}}
                    {{dish_name}}
                {{/each}}
            {{/each}}
        {{/each}}
    </div>
</div>